{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "cloud-course-web"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
          "../node_modules/bootstrap/dist/css/bootstrap.min.css",
          "../node_modules/font-awesome/css/font-awesome.min.css",
          "../node_modules/ionicons/dist/css/ionicons.min.css",
          "../node_modules/select2/dist/css/select2.min.css",
          "../node_modules/nanoscroller/bin/css/nanoscroller.css",
          "../node_modules/owl.carousel/dist/assets/custom.css",
          "../node_modules/owl.carousel/dist/assets/owl.carousel.css",
          "../node_modules/datatables/media/css/dataTables.bootstrap.min.css",
          "../node_modules/bootstrap-datepicker/dist/css/datepicker3.css",
          "../node_modules/bootstrap-datepicker/dist/css/bootstrap-timepicker.css",
          "../node_modules/bootstrap-datepicker/dist/css/daterangepicker.css",
          "../node_modules/admin-lte/dist/css/AdminLTE.min.css",
          "../node_modules/admin-lte/dist/css/skins/skin-blue.min.css",
          "styles.css"
      ],
      "scripts": [
          "../node_modules/jquery/dist/jquery.min.js",
          "../node_modules/bootstrap/dist/js/bootstrap.min.js",
          "../node_modules/admin-lte/dist/js/app.min.js",
          "../node_modules/moment/src/moment.js",
          "../node_modules/bootstrap-datepicker/dist/js/daterangepicker.js",
          "../node_modules/bootstrap-datepicker/dist/js/bootstrap-datepicker.js",
          "../node_modules/bootstrap-datepicker/dist/js/bootstrap-timepicker.js",
          "../node_modules/datatables/media/js/jquery.dataTables.js",
          "../node_modules/datatables/media/js/dataTables.bootstrap.min.js",
          "../node_modules/select2/dist/js/select2.min.js",
          "../node_modules/nanoscroller/bin/javascripts/jquery.nanoscroller.js",
          "../node_modules/owl.carousel/dist/owl.carousel.min.js"
        ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
